{
  "name": "@jimmypruitt/prettier-config",
  "version": "1.0.1",
  "description": "Jimmy's Prettier config",
  "main": ".prettierrc.json",
  "author": "Jimmy Pruitt",
  "license": "MIT",
  "dependencies": {
    "prettier": "^2.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "husky": "^4.2.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
